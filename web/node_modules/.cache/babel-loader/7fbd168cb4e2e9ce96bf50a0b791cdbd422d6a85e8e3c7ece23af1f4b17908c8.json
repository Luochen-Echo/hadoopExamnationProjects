nt-md-center\">\r\n            <div class=\"col-3\">\r\n                <form @submit.prevent=\"login\">\r\n                    <div class=\"mb-3\">\r\n                        <label for=\"username\" class=\"form-label\">用户名</label>\r\n                        <input v-model=\"username\" type=\"text\" class=\"form-control\" id=\"username\" placeholder=\"请输入用户名\">\r\n                    </div>\r\n                    <div class=\"mb-3\">\r\n                        <label for=\"password\" class=\"form-label\">密码</label>\r\n                        <input v-model=\"password\" type=\"password\" class=\"form-control\" id=\"password\" placeholder=\"请输入密码\">\r\n                    </div>\r\n                    <div class=\"error-message\">{{ error_message }}</div>\r\n                    <button type=\"submit\" class=\"btn btn-primary\">提交</button>\r\n                </form>\r\n            </div>\r\n        </div>\r\n    </ContentField>\r\n</template>\r\n\r\n<script>\r\nimport ContentField from '../../../components/ContentField.vue'\r\nimport { useStore } from 'vuex'\r\nimport { ref } from 'vue'\r\nimport router from '../../../router/index'\r\n\r\nexport default {\r\n    components: {\r\n        ContentField\r\n    },\r\n    setup() {\r\n        const store = useStore();\r\n        let username = ref('');\r\n        let password = ref('');\r\n        let error_message = ref('');\r\n\r\n        const jwt_token = localStorage.getItem(\"jwt_token\");\r\n        if (jwt_token) {\r\n            store.commit(\"updateToken\", jwt_token);\r\n            store.dispatch(\"getinfo\", {\r\n                success() {\r\n                    router.push({ name: \"home\" });\r\n                    store.commit(\"updatePullingInfo\", false);\r\n                },\r\n                error() {\r\n                    store.commit(\"updatePullingInfo\", false);\r\n                }\r\n            })\r\n        } else {\r\n            store.commit(\"updatePullingInfo\", false);\r\n        }\r\n\r\n        const login = () => {\r\n            error_message.value = \"\";\r\n            store.dispatch(\"login\", {\r\n                username: username.value,\r\n                password: password.value,\r\n                success() {\r\n                    store.dispatch(\"getinfo\", {\r\n                        success() {\r\n                            router.push({ name: 'home' });\r\n                        }\r\n                    })\r\n                },\r\n                error() {\r\n                    error_message.value = \"用户名或密码错误\";\r\n                }\r\n            })\r\n        }\r\n\r\n        return {\r\n            username,\r\n            password,\r\n            error_message,\r\n            login,\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\nbutton {\r\n    width: 100%;\r\n}\r\ndiv.error-message {\r\n    color: red;\r\n}\r\n.custom-content-field {\r\n  /* 自定义样式 */\r\n  /* 示例：修改宽度和位置 */\r\n  width: 50%;\r\n  margin-top: ;\r\n}\r\n\r\n</style>\r\n"],"mappings":";AAsBA,OAAOA,YAAW,MAAO,sCAAqC;AAC9D,SAASC,QAAO,QAAS,MAAK;AAC9B,SAASC,GAAE,QAAS,KAAI;AACxB,OAAOC,MAAK,MAAO,uBAAsB;AAEzC,eAAe;EACXC,UAAU,EAAE;IACRJ;EACJ,CAAC;EACDK,KAAKA,CAAA,EAAG;IACJ,MAAMC,KAAI,GAAIL,QAAQ,CAAC,CAAC;IACxB,IAAIM,QAAO,GAAIL,GAAG,CAAC,EAAE,CAAC;IACtB,IAAIM,QAAO,GAAIN,GAAG,CAAC,EAAE,CAAC;IACtB,IAAIO,aAAY,GAAIP,GAAG,CAAC,EAAE,CAAC;IAE3B,MAAMQ,SAAQ,GAAIC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC;IACnD,IAAIF,SAAS,EAAE;MACXJ,KAAK,CAACO,MAAM,CAAC,aAAa,EAAEH,SAAS,CAAC;MACtCJ,KAAK,CAACQ,QAAQ,CAAC,SAAS,EAAE;QACtBC,OAAOA,CAAA,EAAG;UACNZ,MAAM,CAACa,IAAI,CAAC;YAAEC,IAAI,EAAE;UAAO,CAAC,CAAC;UAC7BX,KAAK,CAACO,MAAM,CAAC,mBAAmB,EAAE,KAAK,CAAC;QAC5C,CAAC;QACDK,KAAKA,CAAA,EAAG;UACJZ,KAAK,CAACO,MAAM,CAAC,mBAAmB,EAAE,KAAK,CAAC;QAC5C;MACJ,CAAC;IACL,OAAO;MACHP,KAAK,CAACO,MAAM,CAAC,mBAAmB,EAAE,KAAK,CAAC;IAC5C;IAEA,MAAMM,KAAI,GAAIA,CAAA,KAAM;MAChBV,aAAa,CAACW,KAAI,GAAI,EAAE;MACxBd,KAAK,CAACQ,QAAQ,CAAC,OAAO,EAAE;QACpBP,QAAQ,EAAEA,QAAQ,CAACa,KAAK;QACxBZ,QAAQ,EAAEA,QAAQ,CAACY,KAAK;QACxBL,